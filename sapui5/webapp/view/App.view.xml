    <Shell id="shellMain">
        <App id="appMain">
            <pages>
                <Page
                    id="pageMain"
                    title="{i18n>appTitle}"
                >
                    <headerContent>
                        <Button
                            id="btnHeaderDialog"
                            icon="sap-icon://action"
                            press=".onOpenDialogHeader"
                        />
                    </headerContent>
                    <content>
                        <mvc:XMLView
                            id="helloPanelEmbeddedView"
                            viewName="logalilaboratorio.sapui5.view.HelloPanel"
                        />
                        <mvc:XMLView
                            id="invoicesListView"
                            viewName="logalilaboratorio.sapui5.view.InvoicesList"
                        />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
